<h1>Login</h1>

<%= form_for @user, :url => create_path  do |f| %>
<table>
  <tr>
    <td><%= f.label :name%></td>
    <td><%= f.text_field :name%></td>
  </tr>
<tr>
    <td><%= f.label :password%></td>
    <td><%= f.password_field :password%></td>
</tr>
<tr>
<td><%= f.submit value= 'Sign In'%></td>
</tr>
</table>
<% end %>
