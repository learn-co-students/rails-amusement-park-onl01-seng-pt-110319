

<h1>Name: <%= @user.name %></h1>
<h3>Id: <%= @user.id %></h3>
<h3>Mood: <%= @user.mood %></h3>
<h3>Height: <%= @user.height %> inches</h3>
<h3>Tickets: <%= @user.tickets %></h3>

<% flash.each do |type, msg| %>
    <div>
      <%= msg %>
    </div>
  <% end %>

<p><%= link_to "See attractions", attractions_path %></p>

<% if @user.admin %>
    <h3> ADMIN </h3>
<% end %>
<%= link_to "Log Out", "/logout" %> 